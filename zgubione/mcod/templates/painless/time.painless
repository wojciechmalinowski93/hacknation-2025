{% extends 'painless/base.painless'%}
{% block content %}

{{col}} = {{col}}.toString();
if ({{col}} instanceof String) {
    if ({{col}} ==~ /(0[0-9]|1[0-9]|2[0-3]):(0\d|[0-5]\d)(:(0\d|[0-5]\d))?/) {
        return false
    } else {
        return true
    }
} else {
    return true
}
{% endblock %}
